{
  "name": "gemini-cli-gui-wrapper",
  "version": "0.1.0",
  "description": "GUI wrapper for Gemini CLI - AI-powered file management for non-programmers",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\""
  },
  "keywords": [
    "gemini",
    "cli",
    "gui",
    "electron",
    "ai",
    "agent",
    "file-management"
  ],
  "author": "Gemini CLI GUI Wrapper Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/gemini-cli-gui-wrapper.git"
  },
  "dependencies": {
    "electron": "^28.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "@playwright/test": "^1.40.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
